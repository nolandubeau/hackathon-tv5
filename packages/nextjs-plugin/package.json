{
  "name": "@agent-ready-web/nextjs-plugin",
  "version": "0.1.0",
  "description": "Next.js plugin for seamless Agent-Ready Web (ARW) integration",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./runtime": {
      "require": "./dist/runtime.js",
      "import": "./dist/runtime.mjs",
      "types": "./dist/runtime.d.ts"
    },
    "./components": {
      "require": "./dist/components.js",
      "import": "./dist/components.mjs",
      "types": "./dist/components.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "vitest",
    "test:coverage": "vitest --coverage"
  },
  "keywords": [
    "nextjs",
    "arw",
    "agent-ready-web",
    "ai-agents",
    "llm",
    "plugin",
    "webpack",
    "machine-readable"
  ],
  "author": "Nolan Dubeau",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nolandubeau/agent-ready-web.git",
    "directory": "packages/nextjs-plugin"
  },
  "peerDependencies": {
    "next": ">=13.0.0",
    "react": ">=18.0.0"
  },
  "dependencies": {
    "@babel/parser": "^7.24.0",
    "@babel/traverse": "^7.24.0",
    "@babel/types": "^7.24.0",
    "cheerio": "^1.0.0-rc.12",
    "glob": "^10.3.0",
    "turndown": "^7.1.2"
  },
  "bin": {
    "arw-generate": "./dist/cli.js"
  },
  "optionalDependencies": {
    "@agent-ready-web/cli": "^0.1.0",
    "chokidar": "^3.6.0"
  },
  "devDependencies": {
    "@types/babel__traverse": "^7.20.5",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/turndown": "^5.0.4",
    "next": "^14.0.0",
    "react": "^18.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  }
}
